{
  "index_name": "nxu-tzgg-search",
  "sitemap_urls": ["https://nxu-tzsearch.thisish.cn/test.xml"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": "",
      "defaultValue": "Documentation"
    },
    "lvl1": ".ar_article_box h5",
    "lvl2": {
      "selector": "ar_article p a",
      "global": true
    },
    "text": {
      "selector": "ar_article p, .ar_article span",
      "global": true
    }
  },
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}
